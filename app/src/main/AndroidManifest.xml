<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.rayzz.uwataloner">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StudyAloneViewResultsActivity" />
        <activity android:name=".FindCourseCodeActivity" />
        <activity android:name=".AuditCourseChooseSubjectActivity" />
        <activity android:name=".AuditCourseChooseCourseActivity" />
        <activity android:name=".AuditCourseViewResultsActivity" />
        <activity android:name=".StudyAloneChooseBuildingActivity" />
        <activity android:name=".ErrorActivity"></activity>
    </application>

</manifest>